<?xml version="1.0" encoding="utf-8"?>
<resources>
    <attr name="jColorPrimary" />

    <declare-styleable name="Cell">
        <attr name="cellTitle" format="string" />
        <attr name="cellSubject" format="string" />
        <attr name="cellIcon" format="reference" />
        <attr name="cellFitIcon" format="boolean" />
    </declare-styleable>
    <declare-styleable name="CellGroup">
        <attr name="cellGroupTitle" format="string" />
    </declare-styleable>
    <declare-styleable name="CheckedCell">
        <attr name="checkedCellTitle" format="string" />
        <attr name="checkedCellChecked" format="boolean" />
        <attr name="checkedCellCheckedColor" format="color" />
    </declare-styleable>
    <declare-styleable name="RadioCell">
        <attr name="radioCellTitle" format="string" />
        <attr name="radioCellOn" format="boolean" />
        <attr name="radioCellOnColor" format="color" />
    </declare-styleable>

    <declare-styleable name="NavGrid">
        <attr name="navGridRowNumber" format="integer" />
        <attr name="navGridShowDivider" format="boolean" />
    </declare-styleable>
    <declare-styleable name="NavGridItem">
        <attr name="navGridItemTitle" format="string" />
        <attr name="navGridItemIcon" format="reference" />
    </declare-styleable>

    <declare-styleable name="TabBarItem">
        <attr name="tabBarItemTitle" format="string" />
        <attr name="tabBarItemIcon" format="reference" />
        <attr name="tabBarItemSelectedIcon" format="reference" />
    </declare-styleable>

    <declare-styleable name="GridRadioGroup">
        <attr name="gridRadioGroupCheckedColor" format="color" />
    </declare-styleable>

    <declare-styleable name="Spinner">
        <attr name="spinnerLayoutId" format="reference" />
        <attr name="spinnerNumber" format="integer" />
        <attr name="spinnerMaxNumber" format="integer" />
        <attr name="spinnerMiniNumber" format="integer" />
    </declare-styleable>

    <declare-styleable name="JButton">
        <attr name="jButtonTitle" format="string" />
        <attr name="jButtonIcon" format="reference" />
        <attr name="jButtonRadius" format="dimension" />
        <attr name="jButtonTextSize" format="dimension" />
        <attr name="jButtonColor" format="color" />
        <attr name="jButtonTextColor" format="color" />
        <attr name="jButtonStyle" format="enum">
            <enum name="fill" value="0" />
            <enum name="stroke" value="1" />
            <enum name="chip" value="2" />
        </attr>
        <attr name="jButtonSize" format="enum">
            <enum name="normal" value="0" />
            <enum name="little" value="1" />
            <enum name="chip" value="2" />
        </attr>
    </declare-styleable>

    <declare-styleable name="ViewTab">
        <attr name="viewTabOrientation" format="enum">
            <enum name="horizon" value="0" />
            <enum name="vertical" value="1" />
        </attr>
        <attr name="viewTabIdcGravity" format="enum">
            <enum name="top" value="0" />
            <enum name="bottom" value="1" />
        </attr>
        <attr name="viewTabIdcColor" format="color" />
        <attr name="viewTabIdcHPadding" format="dimension" />
        <attr name="viewTabIdcVPadding" format="dimension" />
        <attr name="viewTabIdcSize" format="dimension" />
        <attr name="viewTabIdcScale" format="fraction" />
    </declare-styleable>

    <declare-styleable name="TextTab">
        <attr name="textTabTitles" format="reference" />
    </declare-styleable>

    <declare-styleable name="WaveView">
        <attr name="waveViewHorizon" format="fraction" />
        <attr name="waveViewTop" format="fraction" />
    </declare-styleable>
</resources>